      </filter>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%level %date --%identity --%username %line %method %location %message %newline" />
      </layout>
    </appender>
    <appender name="log4javascript" type="log4net.Appender.RollingFileAppender">
